{
  "manifest_version": 2,
    "name": "GetFlickrImageUrl",
    "description": "Get flickr image url.",
    "version": "1.4",

    "browser_action": {
      "default_icon": {
        "19": "icon19.png",
        "38": "icon38.png"
      },
      "default_popup": "popup.html"
    },
    "permissions": [
      "*://*.flickr.com/"
      ]
}
